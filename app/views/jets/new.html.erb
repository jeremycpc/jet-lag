<h1 class="text-center">New Jet</h1>
<div class="container">
  <%= simple_form_for @jet   do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :has_service, as: :select %>
  <%= f.association :airport_origin %>
  <%= f.association :airport_destination %>
  <%= f.input :max_capacity, as: :select, collection: 1..10 %>
  <%= f.input :start_on, as: :date  %>
  <%= f.input :end_on, as: :date %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add a Jet", class: "btn btn-primary" %>
<% end %>
 <%= link_to "Back home", jets_path %>
</div>
